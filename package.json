{
  "name": "mdast-extension-myst-target",
  "version": "0.0.1",
  "description": "Extension to support the MyST target syntax in mdast (`(name)=`)",
  "license": "MIT",
  "author": "Chris Sewell <https://github.com/chrisjsewell>",
  "keywords": [
    "mdast",
    "unist",
    "myst",
    "micromark"
  ],
  "type": "module",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build:types": "tsc",
    "build:bundle": "rollup -c",
    "build": "npm-run-all -l clean -p build:types build:bundle",
    "prepublishOnly": "npm run build",
    "lint:type-cov": "npm run build:types && type-coverage",
    "lint": "eslint -c .eslintrc.yml --max-warnings 1 {src,tests}/**/*.js",
    "lint:fix": "eslint -c .eslintrc.yml --fix src/**/*.js",
    "format": "prettier --write \"{src,tests}/**/*.{js,ts}\"",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "dependencies": {
    "micromark-factory-space": "^1.0.0",
    "micromark-util-character": "^1.1.0",
    "micromark-util-symbol": "^1.0.1",
    "uvu": "^0.5.3"
  },
  "peerDependencies": {
    "micromark": "^3.0.10"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/jest": "^27.4.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^38.0.7",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "mdast-util-from-markdown": "^1.2.0",
    "micromark-util-types": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "type-coverage": "^2.21.1"
  }
}
