{
    "name": "@unified-myst/target-extension",
    "version": "0.0.2",
    "description": "Extension to support the MyST target syntax (`(name)=`) in unified",
    "license": "MIT",
    "author": "Chris Sewell <https://github.com/chrisjsewell>",
    "keywords": [
        "myst",
        "unified",
        "mdast",
        "micromark"
    ],
    "type": "module",
    "module": "./dist/module/index.js",
    "types": "./dist/types/index.d.ts",
    "files": [
        "src",
        "dist"
    ],
    "exports": {
        "default": {
            "import": "./dist/module/index.js"
        },
        "development": {
            "import": "./src/index.js"
        }
    },
    "scripts": {
        "clean": "rimraf dist",
        "build:types": "tsc",
        "build:bundles": "rollup -c",
        "build": "npm-run-all -l clean -p build:types build:bundles",
        "prepublishOnly": "npm run build",
        "lint:type-cov": "npm run build:types && type-coverage",
        "lint": "eslint --max-warnings 1 {src,tests}/**/*.{js,ts}",
        "lint:fix": "eslint --fix src/**/*.{js,ts}",
        "format": "prettier --write \"{src,tests}/**/*.{js,ts}\"",
        "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js"
    },
    "dependencies": {
        "micromark-factory-space": "^1.0.0",
        "micromark-util-character": "^1.1.0",
        "micromark-util-symbol": "^1.0.1",
        "uvu": "^0.5.3"
    },
    "peerDependencies": {
        "micromark": "^3.0.10"
    },
    "devDependencies": {
        "mdast-util-from-markdown": "^1.2.0",
        "micromark-util-types": "^1.0.2"
    }
}
